{
  "app": {
    "name": "IPv6 WireGuard Manager",
    "version": "3.0.0",
    "debug": false,
    "environment": "development"
  },
  "api": {
    "v1_str": "/api/v1",
    "secret_key": "your-secret-key-change-in-production",
    "access_token_expire_minutes": 60
  },
  "server": {
    "host": "${SERVER_HOST}",
    "port": 8000
  },
  "database": {
    "url": "sqlite:///./ipv6_wireguard.db",
    "host": "localhost",
    "port": 3306,
    "user": "ipv6wgm",
    "password": "password",
    "name": "ipv6wgm",
    "pool_size": 10
  },
  "redis": {
    "url": null,
    "port": 6379,
    "pool_size": 10
  },
  "logging": {
    "level": "INFO",
    "file": null,
    "format": "json"
  },
  "wireguard": {
    "config_dir": "/etc/wireguard",
    "clients_dir": "/etc/wireguard/clients"
  }
}